{
  "name": "mockform",
  "displayName": "Mock Form",
  "description": "A VS Code extension to generate form initial values",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:mockform.generateFormInitialValues"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mockform.generateFormInitialValues",
        "title": "生成form表单初始配置"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "eslint": "^7.32.0",
    "glob": "^7.1.6",
    "mocha": "^8.4.0",
    "vscode-test": "^1.6.0"
  }
}
