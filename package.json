{
  "name": "realproxyaddress",
  "displayName": "realProxyAddress",
  "description": "展示Vue代理后的实际地址",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "extensions": [
          ".js"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node ./node_modules/vscode/bin/compile",
    "watch": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext js",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/vscode": "^1.60.0",
    "eslint": "^7.32.0",
    "typescript": "^4.4.0",
    "vscode-test": "^1.6.1"
  }
}
